<a href="#" class="popup-gallery {{{id}}}">{{{popup_link_text}}}</a>
<script type="application/javascript">
    jQuery("body").ready(function() {


        jQuery(".popup-gallery.{{{id}}}").magnificPopup({
            type: 'image',
            items: [
            {{#images}}
                {
                    src: '{{{image_url}}}'
                },
            {{/images}}
            ],
            gallery: {
                enabled: true
            }
        });
    });
</script>